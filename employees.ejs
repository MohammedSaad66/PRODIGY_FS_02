<h2>Employees</h2>
<a href="/employees/add">Add Employee</a>
<table border="1" cellpadding="5">
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Position</th>
    <th>Department</th>
    <th>Salary</th>
    <th>Actions</th>
  </tr>
  <% employees.forEach(emp => { %>
    <tr>
      <td><%= emp.name %></td>
      <td><%= emp.email %></td>
      <td><%= emp.position %></td>
      <td><%= emp.department %></td>
      <td><%= emp.salary %></td>
      <td>
        <a href="/employees/edit/<%= emp.id %>">Edit</a> |
        <a href="/employees/delete/<%= emp.id %>">Delete</a>
      </td>
    </tr>
  <% }) %>
</table>
<a href="/">Home</a>
